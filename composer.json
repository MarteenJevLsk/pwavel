{
    "name": "marteen/pwavel",
    "description": "Creates a simple PWA support for Laravel Starter Kit.",
    "type": "library",
    "require": {
        "illuminate/support": ">=5.2",
        "php": ">=5.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Marteen\\Pwavel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "marteen"
        }
    ],
    "extra": {
      "laravel": {
        "providers": [
          "Marteen\\Pwavel\\Providers\\PwavelServiceProvider"
        ],
        "aliases": {
          "Pwavel": "Marteen\\Pwavel"
        }
      }
    },
    "minimum-stability": "stable"
}
